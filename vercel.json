{
  "version": 2,
  "name": "yakymenko-portfolio",
  "buildCommand": "GITHUB_PAGES=true npm run build",
  "installCommand": "npm ci",
  "outputDirectory": "out",
  "trailingSlash": false,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
